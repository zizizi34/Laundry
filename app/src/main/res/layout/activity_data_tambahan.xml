<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Tambahan.DataTambahan"
    android:layout_margin="8dp">

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/rvDATA_TAMBAHAN"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:listitem="@layout/card_data_tambahan"
        android:layout_marginHorizontal="8dp"
        />


    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/fab_DATA_TAMBAH_TAMBAH"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/tambahan"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginEnd="32dp"
        android:layout_marginBottom="32dp"
        android:backgroundTint="@color/white"
        android:contentDescription="Tambah Pelanggan"
        />

</androidx.constraintlayout.widget.ConstraintLayout>